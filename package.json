    "r3f",
    "webgl"
  ],
  "peerDependencies": {
    "@react-three/fiber": "8.x",
    "react": "18.x",
    "three": "^0.159.0"
  },
  "devDependencies": {
    "@react-three/fiber": "8.15.11",
    "@types/react": "18.2.40",
